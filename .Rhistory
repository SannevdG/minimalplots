#creating scatterplot with theme_minimal as default
minimal_scatter <- function(data, x, y, plot_title, x_label, y_label) {
p <- ggplot(data, aes_string(x = x, y = y)) +
geom_point() +
ggtitle(plot_title) +
xlab(x_label) +
ylab(y_label) +
theme_minimal()
return(p)
}
minimal_scatter(data, "x", "y", "Test Scatter Plot", "X-Axis", "Y-Axis")
