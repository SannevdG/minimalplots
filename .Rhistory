#creating line graph with theme_minimal as default
minimal_line <- function(data, x, y, plot_title, x_label, y_label) {
p <- ggplot(data, aes_string(x = x, y = y)) +
geom_line() +
ggtitle(plot_title) +
xlab(x_label) +
ylab(y_label) +
theme_minimal()
return(p)
}
data <- data.frame(x = 1:10, y = c(3, 5, 2, 8, 7, 4, 6, 8, 9, 10))
minimal_line(data, "x", "y", "Test Line Plot", "X-Axis", "Y-Axis")
